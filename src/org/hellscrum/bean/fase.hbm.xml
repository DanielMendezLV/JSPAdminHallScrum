<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.hellscrum.bean.fase" table="fase">
		<id name="idfase" type="java.lang.Integer">
			<column name="idfase"></column>
			<generator class="assigned"></generator>
		</id>
		<property name="nombre" type="java.lang.String">
			<column name="nombre" />
		</property>
		<property name="fechainicio" type="java.sql.Date">
			<column name="fechainicio" />
		</property>
		<property name="fechafinalizacion" type="java.sql.Date">
			<column name="fechafinalizacion" />
		</property>
		<property name="idproyecto" type="java.lang.Integer">
			<column name="idproyecto" />
		</property>
		
	</class>
</hibernate-mapping>