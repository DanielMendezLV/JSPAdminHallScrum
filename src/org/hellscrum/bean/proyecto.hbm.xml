<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.hellscrum.bean.proyecto" table="proyecto">
		<id name="idproyecto" type="java.lang.Integer">
			<column name="idproyecto"></column>
			<generator class="assigned"></generator>
		</id>
		<property name="nombre" type="java.lang.String">
			<column name="nombre" />
		</property>
		
		<property name="fechacreacion" type="java.sql.Date">
			<column name="fechacreacion" />
		</property>
		
		<property name="idequipo" type="java.lang.Integer">
			<column name="idequipo" />
		</property>
		
	</class>
</hibernate-mapping>